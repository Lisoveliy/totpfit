{
    "configVersion": "v3",
    "app": {
        "appId": 25087,
        "appName": "totpfit",
        "appType": "app",
        "version": {
            "code": 1,
            "name": "1.3.1"
        },
        "icon": "icon.png",
        "vender": "zepp",
        "description": "Another 2FAuthenticator based on TOTP for Zepp OS"
    },
    "permissions": ["data:os.device.info", "device:os.local_storage"],
    "runtime": {
        "apiVersion": {
            "compatible": "3.0.0",
            "target": "3.0.0",
            "minVersion": "3.0"
        }
    },
    "targets": {
        "default": {
            "module": {
                "page": {
                    "pages": ["page/index", "page/tip"]
                },
                "app-side": {
                    "path": "app-side/index"
                },
                "setting": {
                    "path": "setting/index"
                }
            },
            "platforms": [
                {
                    "st": "s",
                    "dw": 390
                }
            ]
        }
    },
    "i18n": {
        "en-US": {
            "appName": "TOTPFit"
        }
    },
    "defaultLanguage": "en-US"
}
